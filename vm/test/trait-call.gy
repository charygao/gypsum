trait Foo
  def m = 12

class Bar <: Foo

def f(foo: Foo) = foo.m

def g(bar: Bar) = bar.m

def main =
  let bar = Bar()
  if (f(bar) != 12)
    throw Exception()
  if (g(bar) != 12)
    throw Exception()
